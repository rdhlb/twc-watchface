{
  "name": "twc-watchface",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "@fitbit/sdk": "~4.1.0",
    "@fitbit/sdk-cli": "^1.7.2",
    "fitbit-sdk-types": "^4.3.8",
    "prettier": "^2.0.5"
  },
  "fitbit": {
    "appUUID": "35906281-c11a-4cc8-bf3d-8db5d99cac75",
    "appType": "clockface",
    "appDisplayName": "Twc Watchface",
    "iconFile": "resources/icon.png",
    "wipeColor": "#ffffff",
    "requestedPermissions": [
      "access_internet",
      "access_location",
      "run_background",
      "access_user_profile",
      "access_calendar"
    ],
    "buildTargets": [
      "meson",
      "gemini",
      "mira"
    ],
    "i18n": {},
    "defaultLanguage": "en-US"
  },
  "scripts": {
    "build": "fitbit-build",
    "debug": "fitbit",
    "prettier-format": "prettier --config .prettierrc '{app,common,companion,settings}/**/*.{ts,js,tsx,jsx}' --write"
  }
}
